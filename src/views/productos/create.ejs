<h1>Crear Producto</h1>

<form class="form" action="/productos" method="post">
  <div class="form__group">
    <label class="form__label" for="nombre">Nombre:</label>
    <input class="form_input" type="text" name="name" id="nombre" />
    <!-- En el imput en el CRUD name="nombre" pero con sqlite name='name' -->
  </div>
  <button class="button button--primary" type="submit">Nuevo Producto</button>
</form>
